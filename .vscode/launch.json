  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Debug in Firefox",
      "url": "http://localhost:5000/",
      "webRoot": "${workspaceFolder}/public",
      "pathMappings": [
        {
          "url": "http://localhost:5000/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    }
  ]
}
